@model IEnumerable<NewsletterAPI.Data.Models.SendNewsletterLog>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SendStatus)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SendTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ReceiveTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ViewStatus)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NewsletterId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PersonnelId)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SendStatus)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SendTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ReceiveTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ViewStatus)
            </td>
           
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
